form.d-flex.mt-2(role='search')
    a.btn.btn-style(href='/approvedlist') Back
.container-fluid.form-style
    form#signup.form(action='/produce/available?id='+item._id name='registration' method='POST')
        h3 Change Produce Availability 
        hr
        .row
            .col
                .form-field
                    label(for='availability') Produce Availability:
                    select(name='availability')
                        option(value="available" selected = item.availability === "available") Available
                        option(value="booked" selected = item.availability === 'booked') Booked
                        option(value="N/A" selected = item.availability === 'N/A') N/A
       
            .form-field
                input.btn.register-btn(type='submit' value='Register')
script(src='bootstrap-js/js/bootstrap.bundle.js')
